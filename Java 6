public class Main {
	public static void main(String[] args) {	
		
       Random rasgele = new Random();
       
       int sayi;
       int[] sayilar = new int[500];
       for(int d =1;d<500;d++){
    	   sayi = rasgele.nextInt(500);
    	   sayilar[d] = sayi;
       }
       Arrays.sort(sayilar);//sÄ±rala
       
        for(int d =0;d<sayilar.length;d++){
        	System.out.println(sayilar[d]);
        }
}
}
