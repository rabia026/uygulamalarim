class Program
    {
        static void Main(string[] args)
        {
            // dizinin elemanlarını dısardan okutmak.
            int[] a = new int[5];
           
            for (int i = 0; i < 5; i++)
            {
                Console.Write("a[{0}] degeri = ", i);
                a[i] = Convert.ToInt32(Console.ReadLine());
            }
            for (int i = 0; i < 5; i++)
            {
                Console.WriteLine("a'nın {0} degeri ={1}", i, a[i]); 
            }
            Console.ReadKey();
        }
    }
